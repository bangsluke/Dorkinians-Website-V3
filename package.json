{
	"name": "dorkinians-website-v3",
	"version": "1.1.28",
	"description": "Dorkinians FC Statistics Website",
	"private": true,
	"scripts": {
		"dev": "cross-env CONSOLE_LOG_LEVEL=info next dev",
		"dev:debug": "cross-env CONSOLE_LOG_LEVEL=debug next dev",
		"dev:quiet": "cross-env CONSOLE_LOG_LEVEL=error next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "tsc --noEmit",
		"apply-schema": "node scripts/apply-schema.js",
		"sync-config": "cd ../database-dorkinians && npm run sync-config",
		"test:all": "node __tests__/scripts/test-all.js",
		"test:all:debug": "node __tests__/scripts/test-all.js --debug",
		"test:unit": "jest --testPathPattern=unit",
		"test:debug": "cross-env JEST_VERBOSE=true jest --verbose",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:integration": "jest --testPathPattern=integration",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:headless": "playwright test --headed=false",
		"test:e2e:show-last-report": "playwright show-report __tests__/e2e/playwright-report",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:email": "node __tests__/e2e/scripts/test-e2e-email-report.js",
		"test:e2e:navigation": "playwright test __tests__/e2e/01-navigation/navigation.spec.ts",
		"test:e2e:home": "playwright test __tests__/e2e/02-home/home.spec.ts",
		"test:e2e:stats": "playwright test __tests__/e2e/03-stats/stats.spec.ts",
		"test:e2e:totw": "playwright test __tests__/e2e/04-totw/totw.spec.ts",
		"test:e2e:club-info": "playwright test __tests__/e2e/05-club-info/club-info.spec.ts",
		"test:e2e:settings": "playwright test __tests__/e2e/06-settings/settings.spec.ts",
		"test:e2e:api": "playwright test __tests__/e2e/07-api/api.spec.ts",
		"test:e2e:cross-cutting": "playwright test __tests__/e2e/08-cross-cutting/cross-cutting.spec.ts",
		"test:chatbot-report": "node __tests__/scripts/test-chatbot-email-report.js",
		"test:chatbot-report:hide": "node __tests__/scripts/test-chatbot-email-report.js --hide-passed",
		"test:chatbot-report:debug": "cross-env DEBUG_MODE=true node __tests__/scripts/test-chatbot-email-report.js",
		"test:chatbot-report:enhanced-logging": "node __tests__/scripts/test-chatbot-with-enhanced-logging.js",
		"test:chatbot-report:analyze-logs": "node __tests__/scripts/analyze-chatbot-logs.js",
		"test:questions-report": "node __tests__/scripts/test-questions-email-report.js",
		"test:questions-report:hide": "node __tests__/scripts/test-questions-email-report.js --hide-passed"
	},
	"dependencies": {
		"@googlemaps/js-api-loader": "^1.16.6",
		"@googlemaps/markerclusterer": "^2.6.2",
		"@headlessui/react": "^1.7.17",
		"@heroicons/react": "^2.2.0",
		"@netlify/plugin-nextjs": "^5.15.3",
		"@nivo/sankey": "^0.99.0",
		"@types/node": "^20.19.15",
		"@types/nodemailer": "^7.0.4",
		"@types/papaparse": "^5.5.2",
		"@types/react": "^18.3.27",
		"@types/react-dom": "^18.2.18",
		"@upstash/ratelimit": "^2.0.8",
		"@upstash/redis": "^1.36.1",
		"autoprefixer": "^10.4.23",
		"compromise": "^14.14.5",
		"dotenv": "^17.2.3",
		"framer-motion": "^10.16.16",
		"html2canvas": "^1.4.1",
		"natural": "^8.1.0",
		"neo4j-driver": "^5.28.2",
		"next": "^14.2.35",
		"next-auth": "^5.0.0-beta.30",
		"next-pwa": "^5.6.0",
		"node-fetch": "^2.7.0",
		"nodemailer": "^7.0.12",
		"papaparse": "^5.5.3",
		"postcss": "^8.4.32",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-loading-skeleton": "^3.5.0",
		"recharts": "^2.8.0",
		"tailwindcss": "^3.4.19",
		"zod": "^4.3.5",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@netlify/functions": "^2.0.0",
		"@playwright/test": "^1.57.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@types/google.maps": "^3.58.1",
		"@types/jest": "^30.0.0",
		"@types/natural": "^5.1.5",
		"@types/node": "^24.7.0",
		"cross-env": "^10.0.0",
		"eslint": "^9.0.0",
		"eslint-config-next": "^16.0.3",
		"jest": "^30.1.1",
		"prettier": "^3.6.2",
		"ts-jest": "^29.4.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
