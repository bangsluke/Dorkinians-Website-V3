{
	"name": "dorkinians-website-v3",
	"version": "1.1.15",
	"description": "Dorkinians FC Statistics Website",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"sync-config": "cd ../database-dorkinians && npm run sync-config",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "tsc --noEmit",
		"apply-schema": "node scripts/apply-schema.js",
		"test": "jest",
		"test:debug": "cross-env JEST_VERBOSE=true jest --verbose",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:integration": "jest --testPathPattern=integration",
		"test:chatbot-report": "node scripts/test-chatbot-email-report.js",
		"test:chatbot-report:hide": "node scripts/test-chatbot-email-report.js --hide-passed",
		"test:chatbot-report:debug": "cross-env DEBUG_MODE=true node scripts/test-chatbot-email-report.js",
		"test:chatbot-report:enhanced-logging": "node scripts/test-chatbot-with-enhanced-logging.js",
		"test:chatbot-report:analyze-logs": "node scripts/analyze-chatbot-logs.js",
		"test:questions-report": "node scripts/test-questions-email-report.js",
		"test:questions-report:hide": "node scripts/test-questions-email-report.js --hide-passed"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.17",
		"@heroicons/react": "^2.2.0",
		"@netlify/plugin-nextjs": "^5.14.3",
		"@types/node": "^20.19.15",
		"@types/nodemailer": "^7.0.3",
		"@types/papaparse": "^5.3.16",
		"@types/react": "^18.3.26",
		"@types/react-dom": "^18.2.18",
		"autoprefixer": "^10.4.16",
		"compromise": "^14.14.4",
		"dotenv": "^17.2.3",
		"framer-motion": "^10.16.16",
		"natural": "^8.1.0",
		"neo4j-driver": "^5.28.2",
		"next": "14.2.33",
		"next-pwa": "^5.6.0",
		"node-fetch": "^2.7.0",
		"nodemailer": "^7.0.10",
		"openai": "^4.20.1",
		"papaparse": "^5.5.3",
		"postcss": "^8.4.32",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"recharts": "^2.8.0",
		"tailwindcss": "^3.4.18",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@netlify/functions": "^2.0.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@types/jest": "^30.0.0",
		"@types/natural": "^5.1.5",
		"@types/node": "^24.7.0",
		"cross-env": "^10.0.0",
		"eslint": "^8.57.1",
		"eslint-config-next": "14.2.33",
		"jest": "^30.1.1",
		"prettier": "^3.6.2",
		"ts-jest": "^29.4.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
